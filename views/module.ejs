<%
function beautifyCamelCase(string) {
    return string.charAt(0).toUpperCase() + string.slice(1);
}
let palletMethod = `${beautifyCamelCase(section)}:${beautifyCamelCase(method)}`
%>

<a href="/extrinsics/<%= chainID; %>/<%= section; %>/<%= method %>" class="btn btn-outline-primary text-capitalize"><%= palletMethod; %></a>
