<%
let f = contract;
%>
<table class="table table-hover table-bordered">
  <tbody>
    <tr>
      <td width="200">Contract Address</td>
      <td><code><%- uiTool.getShortHash(f.address, false) %></code>
      </td>
    </tr>

    <tr>
      <td width="200">Code Hash</td>
      <td>
        <a href='/wasmcode/<%= f.codeHash %>'><%- uiTool.getShortHash(f.codeHash, false) %></a>
      </td>
    </tr>

    <tr>
      <td>Instantiated</td>
      <td>
	<%= uiTool.presentTS(f.blockTS); %> @ <a href='/blocks/<%= f.chainID; %>'><%= f.chainName; %></a> <%- include("blockNumber", {chainID: f.chainID, blockNumber: f.instantiateBN}); %>
      </td>
    </tr>
    <tr>
      <td>Salt</td>
      <td><%= f.salt %></td>
    </tr>

    <tr>
      <td>Deployer</td>
      <td><%- uiTool.presentID(f.deployer_ss58) %></td>
    </tr>

    <tr>
      <td>Constructor</td>
      <td>
          <code><%- contract.constructor %></code>
      </td>
    </tr>

    <tr>
      <td>WASM</td>
      <td>
          <textarea style='width: 800px; height: 200px' disabled><%- contract.wasm %></textarea>
      </td>
    </tr>
  </tbody>
</table>
