<%- include('header', {chainInfo: chainInfo, title: "XCM Transfers"}); %>

<h5>XCM Transfers Log</h5>


<table id="tablexcmtransferslog" class="display" style='width: 100%'>
      <thead>
        <tr>
          <th><%- include("tooltip", {k: "xcmtransferslogaddts"}) %>Msg Time</th>
          <th><%- include("tooltip", {k: "xcmtransferslogextrinsic"}) %>Origination/Extrinsic</th>
          <th><%- include("tooltip", {k: "iddest"}) %>Destination</th>
          <th><%- include("tooltip", {k: "xcmtransferslogsourcets"}) %>Original Extrinsic Time</th>
          <th><%- include("tooltip", {k: "xcmtransferslogsymbol"}) %>Amount Sent</th>
          <th><%- include("tooltip", {k: "xcminfo"}) %>XCM Info</th>
          <th><%- include("tooltip", {k: "xcmtransferslogdelay"}) %>Delay</th>
        </tr>
      </thead>
</table>

<%- include('footer', {apiUrl: apiUrl}); %>
<script>
let id = '<% id %>';
let chainID_or_chainName = "<%- chainIDorChainName %>";
let chainIDDest_or_chainName = "<%- chainIDDestorChainName %>";

$(document).ready(function() {
    showxcmtransferslog(chainID_or_chainName, chainIDDest_or_chainName);
});
</script>
<script src="/chains.js"></script>
<script src="/chainui.js"></script>
<script src="/xcmtransferslogui.js"></script>
